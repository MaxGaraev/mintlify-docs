#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/gptbots.chat/node_modules/.pnpm/mintlify@4.0.143_openapi-types@12.1.3_react-dom@18.2.0_react@18.2.0/node_modules/mintlify/node_modules:/var/www/gptbots.chat/node_modules/.pnpm/mintlify@4.0.143_openapi-types@12.1.3_react-dom@18.2.0_react@18.2.0/node_modules:/var/www/gptbots.chat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/var/www/gptbots.chat/node_modules/.pnpm/mintlify@4.0.143_openapi-types@12.1.3_react-dom@18.2.0_react@18.2.0/node_modules/mintlify/node_modules:/var/www/gptbots.chat/node_modules/.pnpm/mintlify@4.0.143_openapi-types@12.1.3_react-dom@18.2.0_react@18.2.0/node_modules:/var/www/gptbots.chat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
